{
  "name": "travelagent",
  "version": "1.0.0",
  "description": "Travel Discovery Orchestrator CLI",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "tsx watch src/index.ts",
    "seed-context": "tsx src/context/seed.ts"
  },
  "keywords": [
    "travel",
    "discovery",
    "cli",
    "orchestrator"
  ],
  "author": "Rajan",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@lancedb/lancedb": "^0.23.0",
    "dotenv": "^17.2.3",
    "openai": "^6.15.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
